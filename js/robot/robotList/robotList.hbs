 {{#.}} {{#each rows}}
<li data-id="{{Fid}}" class="contact-item">
	<a href="js/robot/robot.html?id={{Fid}}" class="item-link">
		<div class="item-content">
			<div class="item-media">
				<img src="img/{{DocCategory}}.png" width="44">
			</div>
			<div class="item-inner">
				<div class="item-title-row">
					<div class="item-title">{{Title}} </div>
					<div class="item-after">{{PublishTime}}</div>
				</div>
				<div class="item-text">{{transHrDocumentCategory DocCategory}}</div>
			</div>
		</div>
	</a>
</li>
{{/each}} {{/.}}